{
  "root": true,
  "extends": [
    "next/core-web-vitals",
    "plugin:import/typescript",
    "plugin:prettier/recommended"
  ],
  "plugins": ["import", "prettier"],
  "rules": {
    "prettier/prettier": "warn",
    "import/no-unresolved": "error",
    "import/order": "off"
  },
  "settings": {
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true
      }
    }
  }
}
